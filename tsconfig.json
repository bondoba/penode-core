{
  "compilerOptions": {
    "target": "es6",
    "module": "ES6",
    "lib": [
      "dom"
    ],
    "jsx": "preserve",
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "./dist/",
    "rootDir": "./",
    "composite": true,
    "removeComments": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "strictPropertyInitialization": false,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "types": [
      "node"
    ],
    "forceConsistentCasingInFileNames": true
  }
}
